{
  "name": "gdbots/pbjx",
  "description": "Messaging tools for pbj.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.1",
    "gdbots/pbj": "~1.1",
    "gdbots/query-parser": "~0.2",
    "gdbots/schemas": "~1.1",
    "psr/log": "~1",
    "symfony/event-dispatcher": "~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.3",
    "aws/aws-sdk-php": "~3.18",
    "ruflin/elastica": "~3.2"
  },
  "suggest": {
    "ext-gearman": "Allows you to use GearmanTransport and GearmanConsumer for distributed processing."
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Pbjx\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Pbjx\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
