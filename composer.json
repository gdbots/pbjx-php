{
  "name": "gdbots/pbjx",
  "description": "Messaging tools for pbj.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=5.6",
    "gdbots/pbj": "~1.0",
    "gdbots/schemas": "~1.0",
    "psr/log": "~1",
    "symfony/event-dispatcher": "~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.3",
    "aws/aws-sdk-php": "~3.0"
  },
  "suggest": {
    "ext-gearman": "Allows you to use GearmanTransport and GearmanConsumer for distributed processing."
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Pbjx\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Pbjx\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
